PREFIX sx: <http://shex.io/ns/shex#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix : <http://example.org/>
prefix schema: <http://schema.org/>

<> sx:shapes :User .

:User sx:shapeExprs ( 
   [ sx:nodeKind sx:iri ]
   [ sx:expression [ 
      sx:expressions (
      [ sx:predicate schema:name ;
       sx:valueExpr [ sx:datatype xsd:string ]
      ]
      [ sx:predicate schema:birthDate ;
        sx:valueExpr [ sx:datatype xsd:date ] ;
        sx:min 0
      ]
      [ sx:predicate schema:gender ;
        sx:valueExpr [ sx:values ( schema:Male schema:Female ) ]
      ]
    )
    ] 
   ]
).
