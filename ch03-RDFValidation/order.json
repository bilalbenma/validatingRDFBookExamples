{ "type": "object",
  "properties": {
   "pedido": { "type": "object",
     "properties": { 
       "fecha": { 
         "type": "string", 
         "format": "date-time" },
       "productos": { 
         "type": "array",
         "items": {
           "type": "object",
           "properties": { 
             "código": {
                "type": "string",
                "pattern": "[A-Z]\\d{2}"
             },
             "nombre": {"type": "string"},
             "cantidad": {
               "type": "integer", 
               "minimum": 1 }
           },
           "required": ["código","nombre","cantidad" ]
         },
        "minItems": 1
       }
     },
     "required": ["fecha", "productos"]
   }
 }
}